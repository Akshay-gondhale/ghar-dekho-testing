    <%- include("footer.ejs") %>


    <script src="js/flickity.js"></script>
    <script>
        $('.main-carousel').flickity({
            // options
            cellAlign: 'center',
            contain: true,
            draggable: true,
            wrapAround: true
        });

        function DoFast(str){
            var inputString = str;
            // window.location.href = "/homeInterest"+inputString;
            var href = "/homeInterest" + inputString;
            console.log(href)
            window.location.href = href;
        }
    </script>

</body>

</html>